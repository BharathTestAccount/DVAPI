		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Extract the bearer token from the response cookie\r",
							"var cookieHeader = pm.response.headers.get(\"Set-Cookie\");\r",
							"var bearerToken = null;\r",
							"if (cookieHeader) {\r",
							"  var match = cookieHeader.match(/auth=([^;]+)/);\r",
							"  if (match && match.length > 1) {\r",
							"    bearerToken = match[1];\r",
							"  }\r",
							"}\r",
							"\r",
							"// Set the bearer token as an environment variable\r",
							"if (bearerToken) {\r",
							"  pm.environment.set(\"bearer_token\", bearerToken);\r",
							"}\r",
							""
						],
